  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/testServer.js",
    "test": "npm run test:lint && npm run test:coverage && npm run test:coverage:threshold && npm run test:public:coverage && npm run test:public:coverage:threshold",
    "test:coverage": "nyc --reporter=html mocha --opts ./mocha.opts ./test/lib ./test/*.js && nyc report",
    "test:coverage:threshold": "nyc check-coverage --lines 85 --functions 85 --branches 60",
    "test:backend": "mocha --opts ./mocha.opts ./test/lib ./test/*.js",
    "test:lint": "npm run test:lint:server && npm run test:lint:public && npm run test:lint:jsx",
    "test:lint:server": "eslint --ext .js bin lib *.js test config",
    "test:lint:public": "eslint --ext .js public",
    "test:lint:jsx": "eslint --ext .jsx public test/public",
    "test:public:coverage": "karma start karma.coverage.conf.js",
    "test:public:coverage:threshold": "nyc check-coverage --branches 70 --functions 80 --statements 80 --lines 80 coverage/**/coverage*.json",
    "test:public:debug": "karma start karma.debug.conf.js --browsers=Chrome --single-run=false",
    "test:public": "karma start",
    "test:public:mac": "karma start --browsers=Chrome,Firefox,Safari,Opera",
    "build": "node ./bin/build.js",
    "deploy:staging": "node ./bin/build.js -s staging && npm run deploy:staging:no-build",
    "deploy:staging:no-build": "sls deploy -s staging -r eu-central-1",
    "deploy:production": "node ./bin/build.js -s production && npm run deploy:production:no-build",
    "deploy:production:no-build": "sls deploy -s production -r eu-central-1"
  },
